 <?php
            class Form_Example extends Zend_Form{
                function init() {
     $this->setAction('my/action')
             ->setAttribs(array('class'=>'form', 'id'=>'exemple'))
             ->setMethod('post');
     $title = $this->createElement('text', 'title', array('lable'=>'Title:', 'size'=>35));
     $submit = $this->createElement('submit', 'submit', array('lable'=>'Submit', 'class'=>'submit'));
     $this->addElement($title)->addElement($submit);
     }   
            }
        ?>
